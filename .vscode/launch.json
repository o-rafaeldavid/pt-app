  "version": "0.2.0",
  "configurations": [
    {
      "name": "pt_app",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define-from-file", ".env.local.json"]
    },
    {
      "name": "pt_app (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": ["--dart-define-from-file", ".env.local.json"]
    },
    {
      "name": "pt_app (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": ["--dart-define-from-file", ".env.local.json"]
    }
  ]
}
